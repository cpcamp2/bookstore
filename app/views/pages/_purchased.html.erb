<table class="table table-condensed"> 
  <thead> 
    <tr> 
      <th>Name</th> 
      <th>Purchase Date</th> 
      <th></th> 
    </tr>
  </thead> 
  <tbody>
    <% @purchased.each do |purchase| %>
      <tr> 
        <td><%= link_to purchase.book.name, pickup_path(purchase.uuid) %></td>
        <td><%= time_ago_in_words(purchase.created_at) %> ago </td> 
      </tr> 
    <% end %>
  </tbody> 
</table>